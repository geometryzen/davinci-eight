{
    "name": "davinci-eight",
    "version": "8.4.28",
    "description": "WebGL library for mathematical physics using Geometric Algebra",
    "author": {
        "name": "David Geo Holmes",
        "email": "david.geo.holmes@gmail.com"
    },
    "browser": "dist/umd/index.js",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "system": "dist/system/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/geometryzen/davinci-eight.git"
    },
    "engines": {
        "node": ">=4"
    },
    "bugs": {
        "url": "https://github.com/geometryzen/davinci-eight/issues"
    },
    "license": "MIT",
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/geometryzen/davinci-eight/blob/master/LICENSE"
        }
    ],
    "keywords": [
        "WebGL",
        "STEMCstudio",
        "Geometric",
        "Algebra",
        "Physics",
        "Mathematics"
    ],
    "scripts": {
        "build": "rollup -c",
        "lint": "eslint . --ext '.ts,.tsx'",
        "test": "mocha 'test/**/*.spec.ts'",
        "coverage": "nyc mocha 'test/**/*.spec.ts'",
        "docs": "npm run docs:typedoc && open-cli docs/index.html",
        "docs:typedoc": "trash docs && typedoc --options typedoc.json",
        "pages": "npm run docs:typedoc && gh-pages -d docs",
        "prepublishOnly": "node check_publish_env.js && npm run build",
        "server": "live-server"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^8.4.0",
        "@types/chai": "^4.3.3",
        "@types/mocha": "^9.1.1",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "chai": "^4.3.6",
        "eslint": "^8.22.0",
        "gh-pages": "4.0.0",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "rollup": "^2.78.1",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.10",
        "typescript": "^4.7.4"
    }
}